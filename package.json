{
  "name": "ts-express-oauth",
  "version": "0.0.1",
  "description": "Typescript nodejs express authorisation api server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build:tsc": "tsc -p .",
    "build": "ncc build src/index.ts -o dist",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:tsc": "tsc -w src/index.ts",
    "dev:node": "nodemon --watch dist",
    "test": "jest",
    "tdd": "jest --watch",
    "coverage": "jest --collectCoverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.22",
    "@types/pg": "^7.14.4",
    "@types/supertest": "^2.0.10",
    "@zeit/ncc": "^0.22.3",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dmijatovic/ts-express-oauth.git"
  }
}
